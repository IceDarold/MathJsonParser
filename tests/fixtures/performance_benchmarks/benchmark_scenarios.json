[
  {
    "name": "simple_arithmetic_benchmark",
    "description": "Benchmark for simple arithmetic operations",
    "mathir": {
      "expr_format": "latex",
      "targets": [
        {
          "type": "value",
          "name": "result",
          "expr": "2 + 3 * 4 - 1"
        }
      ],
      "output": {"mode": "exact"}
    },
    "performance_expectations": {
      "max_execution_time": 0.1,
      "max_memory_mb": 10,
      "complexity": "O(1)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "arithmetic",
      "difficulty": "trivial",
      "tags": ["benchmark", "arithmetic", "fast"]
    }
  },
  {
    "name": "polynomial_integration_benchmark",
    "description": "Benchmark for polynomial integration",
    "mathir": {
      "expr_format": "latex",
      "symbols": [{"name": "x", "domain": "R"}],
      "targets": [
        {
          "type": "integral_def",
          "expr": "x^5 + 3*x^4 - 2*x^3 + x^2 - 5*x + 7",
          "var": "x",
          "limits": [0, 1],
          "name": "result"
        }
      ],
      "output": {"mode": "exact"}
    },
    "performance_expectations": {
      "max_execution_time": 0.5,
      "max_memory_mb": 20,
      "complexity": "O(n)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "integration",
      "difficulty": "easy",
      "tags": ["benchmark", "integration", "polynomial"]
    }
  },
  {
    "name": "trigonometric_limit_benchmark",
    "description": "Benchmark for trigonometric limit computation",
    "mathir": {
      "expr_format": "latex",
      "symbols": [{"name": "x", "domain": "R"}],
      "targets": [
        {
          "type": "limit",
          "expr": "\\frac{\\sin(x) - x*\\cos(x)}{x^3}",
          "var": "x",
          "to": "0"
        }
      ],
      "output": {"mode": "exact"}
    },
    "performance_expectations": {
      "max_execution_time": 1.0,
      "max_memory_mb": 30,
      "complexity": "O(log n)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "limits",
      "difficulty": "medium",
      "tags": ["benchmark", "limits", "trigonometric"]
    }
  },
  {
    "name": "finite_sum_benchmark",
    "description": "Benchmark for finite sum computation",
    "mathir": {
