      "targets": [
        {
          "type": "sum",
          "term": "n^3 + 2*n^2 - n + 1",
          "idx": "n",
          "start": "1",
          "end": "1000"
        }
      ],
      "output": {"mode": "exact"}
    },
    "performance_expectations": {
      "max_execution_time": 2.0,
      "max_memory_mb": 50,
      "complexity": "O(n)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "summation",
      "difficulty": "medium",
      "tags": ["benchmark", "summation", "finite"]
    }
  },
  {
