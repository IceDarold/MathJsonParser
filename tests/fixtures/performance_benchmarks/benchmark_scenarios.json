  {
    "name": "system_solving_benchmark",
    "description": "Benchmark for system of equations solving",
    "mathir": {
      "expr_format": "latex",
      "symbols": [
        {"name": "x1", "domain": "R"},
        {"name": "x2", "domain": "R"},
        {"name": "x3", "domain": "R"},
        {"name": "x4", "domain": "R"}
      ],
      "targets": [
        {
          "type": "solve_for",
          "unknowns": ["x1", "x2", "x3", "x4"],
          "equations": [
            "2*x1 + x2 - x3 + x4 = 5",
            "x1 - x2 + 2*x3 - x4 = 1",
            "x1 + x2 + x3 + x4 = 6",
            "x1 - x2 - x3 + 2*x4 = 2"
          ]
        }
      ],
      "output": {"mode": "exact"}
    },
    "performance_expectations": {
      "max_execution_time": 2.0,
      "max_memory_mb": 60,
      "complexity": "O(n^3)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "solving",
      "difficulty": "medium",
      "tags": ["benchmark", "system_solving", "linear"]
    }
  },
  {
    "name": "complex_workflow_benchmark",
    "description": "Benchmark for complex multi-target workflow",
    "mathir": {
      "expr_format": "latex",
      "symbols": [
        {"name": "x", "domain": "R"},
        {"name": "n", "domain": "N"}
      ],
      "definitions": {
        "functions": [
          {
            "name": "f",
            "args": ["x"],
            "expr": "x^2 + \\sin(x)"
          }
        ]
      },
      "targets": [
        {
          "type": "integral_def",
          "expr": "f(x)",
          "var": "x",
          "limits": [0, "\\pi"],
          "name": "I1"
        },
        {
          "type": "sum",
          "term": "\\frac{1}{n^2}",
          "idx": "n",
          "start": "1",
          "end": "100"
        },
        {
          "type": "limit",
          "expr": "\\frac{f(x) - f(0)}{x}",
          "var": "x",
          "to": "0"
        },
        {
          "type": "value",
          "name": "combined",
          "expr": "I1 + sum + limit"
        }
      ],
      "output": {"mode": "decimal", "round_to": 6}
    },
    "performance_expectations": {
      "max_execution_time": 5.0,
      "max_memory_mb": 100,
      "complexity": "O(n^2)"
    },
    "metadata": {
      "category": "performance_benchmarks",
      "subcategory": "complex_workflow",
      "difficulty": "hard",
      "tags": ["benchmark", "multi_target", "complex"]
    }
  },
